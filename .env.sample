# LLM Provider Configuration
# Set to "openrouter" for OpenRouter or "azure" for Azure OpenAI
LLM_PROVIDER=openrouter

# OpenRouter API Configuration
# Get your API key from https://openrouter.ai/
OPENAI_API_KEY=your_openai_api_key_here

# OpenRouter API Base URL
OPENAI_API_BASE=https://openrouter.ai/api/v1

# OpenRouter Model Configuration
# You can specify a different model to use with OpenRouter
OPENROUTER_MODEL=openai/gpt-4o-mini

# Available models:
# - openai/gpt-4o-mini
# - openai/gpt-4
# - anthropic/claude-3-opus
# - anthropic/claude-3-sonnet
# See more at: https://openrouter.ai/docs#models

# Azure OpenAI Configuration (only needed if LLM_PROVIDER=azure)
# AZURE_OPENAI_API_KEY=your_azure_openai_api_key_here
# AZURE_OPENAI_ENDPOINT=https://your-resource-name.openai.azure.com/
# AZURE_OPENAI_API_VERSION=2023-05-15
# AZURE_OPENAI_DEPLOYMENT_ID=gpt-35-turbo-0125 

# Application Configuration
DATA_APP_ENTRYPOINT="crewai_app/orchestrator.py" 